@model IEnumerable<eBookLoginHW1.Models.User>

@{
    ViewData["Title"] = "Admin Dashboard";
}

<h2>All Users’ Credit-Card Info</h2>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Username</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>National ID</th>
            <th>Card Number</th>
            <th>Expiry</th>
            <th>CVC</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var u in Model)
        {
            <tr>
                <td>@u.Username</td>
                <td>@u.FirstName</td>
                <td>@u.LastName</td>
                <td>@u.NationalId</td>
                <td>@u.CreditCardNumber</td>
                <td>@u.CreditCardExpiry</td>
                <td>@u.CreditCardCVC</td>
            </tr>
        }
    </tbody>
</table>
